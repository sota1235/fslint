{
  "name": "fslint",
  "version": "1.1.0",
  "description": "Lint for checking file size.",
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "fslint": "./lib/bin/fslint.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -2",
    "lint:fix": "prettier --write ./src/**/*.ts",
    "lint:test": "eslint test/",
    "lint:src": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "lint": "npm run lint:fix && npm run lint:test && npm run lint:src",
    "test": "mocha --reporter spec --compilers js:@babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sota1235/fslint.git"
  },
  "keywords": [
    "file",
    "size",
    "lint",
    "checker"
  ],
  "author": "sota1235",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/glob": "^5.0.35",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.4",
    "babel-eslint": "^7.0.0",
    "babel-preset-power-assert": "^3.0.0",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "husky": "^1.0.0-rc.14",
    "lint-staged": "^7.2.2",
    "mocha": "^5.2.0",
    "power-assert": "^1.4.1",
    "prettier": "^1.14.2",
    "proxyquire": "^1.7.9",
    "sinon": "^1.17.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@types/cli-color": "^0.3.29",
    "@types/filesize": "^3.6.0",
    "cli-color": "^1.1.0",
    "commander": "^2.18.0",
    "filesize": "^3.3.0",
    "glob": "^7.0.3",
    "lodash": "^4.12.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "git add"
    ]
  }
}
